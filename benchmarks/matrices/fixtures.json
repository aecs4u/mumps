    "header_line_1": "# Comment describing matrix",
    "header_line_2": "# n nnz",
    "header_line_3": "n nnz (dimension and number of non-zeros)",
    "header_line_4": "# i j value",
    "data_lines": "i j value (1-indexed row, column, value triplets)"
